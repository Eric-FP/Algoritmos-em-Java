package aula4;

import java.util.Scanner;

public class ex2 {
	public static void main(String [] agrs) {
		Scanner input = new Scanner(System.in);
		int num[] = new int[10], numuser;
		int cont = 0, qtd = 0;
		System.out.println("Informe 10 numeros : ");
		for (cont = 0; cont < 10; cont++) {
			num[cont] = input.nextInt();
		}
		System.out.println("Informe um numero para procurar dentro do vetor: ");
		numuser = input.nextInt();
		for (cont = 0; cont < 10; cont++) {
			if (num[cont] == numuser) {
				qtd++;
			}
		}
		System.out.println("O numero informado aparece no vetor "+qtd+" vezes");
	}
}
